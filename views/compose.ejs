        <div class="form-group">
            <label>Title</label>
            <input type="text" class="form-control" name="postTitle">
        </div>
        <div class="form-group">
            <label>Post</label>
            <textarea name="postCont" class="form-control" id="" cols="30" rows="10"></textarea>
